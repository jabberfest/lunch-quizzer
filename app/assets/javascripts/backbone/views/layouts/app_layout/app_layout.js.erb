
define([
    'jquery',
    'underscore',
    'backbone',
    'handlebars',
    'marionette',
    '<%= javascript_path('backbone/views/layouts/app_layout/app_layout_template')%>'
], function($,_,Backbone,Handlebars,Marionette){

	var AppLayout = Backbone.Marionette.Layout.extend({
		template: Handlebars.templates['app_layout_template'],
		regions:{
			app_body: '#main_body',
			tab_nav:'#tab_nav',
			lunch_tab: '#lunch_tab',
			event_tab: '#event_tab',
			location_tab: '#location_tab',
			lunches: '#lunches',
			new_event: '#new_event',
			locations: "#locations"
		}

	});

	return AppLayout;

});