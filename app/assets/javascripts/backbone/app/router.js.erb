// Filename: router.js
define([
    'jquery',
    'underscore',
    'backbone',
    'handlebars',
    'marionette',
    '<%= javascript_path('backbone/controllers/tab_controller') %>'
], function($, _, Backbone,Handlebars,Marionette,TabController){

      var MainAppController = {

          Lunches: function() {    
            TabController.setLunchTab();
          }

      };


      var Router = Backbone.Marionette.AppRouter.extend({
            appRoutes : {
                '': 'Lunches',
            }

      });

      return new Router({controller: MainAppController});

});